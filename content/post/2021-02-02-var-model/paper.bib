@article{gaiqingen2015,
  title = {{要素市场扭曲、垄断势力与全要素生产率}},
  author = {{盖庆恩} and {朱喜} and {程名望} and {史清华}},
  year = {2015},
  volume = {50},
  pages = {61--75},
  issn = {0577-9154},
  abstract = {本文从广义视角研究了要素市场扭曲对全要素生产率的影响,强调要素市场扭曲不仅通过影响在位企业的资源配置效率直接降低全要素生产率,而且会通过垄断势力改变企业的进入退出行为间接降低全要素生产率。基于1998\textemdash 2007年中国工业企业数据库进行的实证分析表明:若资本市场扭曲得到改善,样本期间制造业的全要素生产率平均可以提高57.79\%,其中通过直接效应提高31.46\%,间接效应提高26.32\%;若劳动力市场扭曲得到改善,全要素生产率可以提高33.12\%,其中通过直接效应提高11.42\%,间接效应提高21.69\%。最后本文还探讨了劳动力收入份额、国有企业以及企业规模对资源配置效率的影响,证明了模型和结论的稳健性。},
  file = {C\:\\Users\\gale\\OneDrive\\中南财经政法大学\\paper\\全要素生产率\\盖庆恩 et al_2015_要素市场扭曲、垄断势力与全要素生产率.pdf},
  journal = {经济研究},
  keywords = {Entry and Exit Behavior,Markup,Total Factor Productivity,全要素生产率,垄断势力,资源配置不当,进入退出行为 Resource Misallocation},
  language = {中文;},
  number = {05}
}

