
@article{caiyuezhou2017,
  title = {{全要素生产率增长中的技术效应与结构效应\textemdash\textemdash 基于中国宏观和产业数据的测算及分解}},
  author = {{蔡跃洲} and {付一夫}},
  year = {2017},
  volume = {52},
  pages = {72--88},
  issn = {0577-9154},
  abstract = {宏观全要素生产率增长可分解为代表普遍技术进步的技术效应和要素流动配置的结构效应。本文利用中国宏观及产业数据,在增长核算基础上将TFP增长分解为技术效应和结构效应,据以对改革开放以来不同阶段中国经济增长的来源进行细致剖析。实证结果表明:(1)得益于后发优势,1978\textemdash 2014年间中国经济增长整体质量较高,增长动力约1/3来自技术水平的普遍提升,而结构效应的作用仅为技术效应的1/5。(2)2005年以后,中外技术差距的缩小导致后发优势逐步衰减,技术进步对经济增长的支撑作用迅速下降;而结构效应对经济增长的贡献度不断提高,并维持了较高的TFP增长率;该趋势在二、三产业尤为突出,这也是工业化和城市化推进的结果。(3)金融危机后,产能过剩的钢铁、水泥所属领域和"金融与保险"、"房地产"等细分行业技术停滞或倒退却积累了更大比重的要素,存在要素资源配置"逆技术进步倾向";要素驱动特征不断强化,至2014年才出现扭转迹象。宏观和产业TFP增长是未来保持中国经济中高速增长、提高经济增长质量的重要支撑。短期内应着力优化产业结构,将要素资源引导到技术和效率水平更高的细分行业,借助结构效应实现TFP增长;中长期则要实施好创新驱动发展战略,切实推动各行业技术进步。},
  file = {C\:\\Users\\gale\\OneDrive\\中南财经政法大学\\paper\\全要素生产率\\蔡跃洲_付一夫_2017_全要素生产率增长中的技术效应与结构效应——基于中国宏观和产业数据的测算及分解.pdf},
  journal = {经济研究},
  keywords = {Growth Accounting,Structural Effect,Technical Effect,全要素生产率,增长核算 Total Factor Productivity(TFP),技术效应,结构效应},
  language = {中文;},
  number = {01}
}

@article{chenchuanglian2019,
